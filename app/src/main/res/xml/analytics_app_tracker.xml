<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="TypographyDashes">

    <!--  The property id associated with this analytics tracker -->
    <string name="ga_trackingId">UA-80039229-1</string>

    <bool name="ga_debug">true</bool>
    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>
    <!-- Enable automatic app crash-->
    <bool name="ga_reportUncaughtExceptions">true</bool>
    <!-- Percentage of events to include in reports -->
    <string name="ga_sampleFrequency">100.0</string>
    <!-- How long a session exists before giving up -->
    <integer name="ga_sessionTimeout">300</integer>

    <screenName name="com.gsbelarus.gedemin.skeleton.app.view.activity.MainActivity">
        Home Screen
    </screenName>

</resources>